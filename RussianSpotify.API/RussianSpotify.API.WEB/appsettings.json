{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JWT": {
    "ValidAudience": "${JWT_VALID_AUDIENCE}",
    "ValidIssuer": "${JWT_VALID_ISSUER}",
    "Secret": "${JWT_SECRET}"
  },
  "EmailSettings": {
    "FromName": "${EMAIL_FROM_NAME}",
    "EmailAddress": "${EMAIL_ADDRESS}",
    "Password": "${EMAIL_PASSWORD}",
    "SMTPServerHost": "${EMAIL_HOST}",
    "SMTPServerPort": "${EMAIL_PORT}"
  },
  "Authentication": {
    "VK": {
      "AppId": "${VK_ID}",
      "AppSecret": "${VK_SECRET}"
    },
    "Yandex": {
      "AppId": "${YANDEX_ID}",
      "AppSecret": "${YANDEX_SECRET}"
    },
    "Google": {
      "AppId": "${GOOGLE_ID}",
      "AppSecret": "${GOOGLE_SECRET}"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_CONNECTION_STRING}"
  },
  "S3": {
    "AccessKey": "${S3_ACCESS_KEY}",
    "SecretKey": "${S3_SECRET_KEY}",
    "ServiceUrl": "${S3_URL}",
    "BucketName": "${S3_BUCKET_NAME}",
    "IgnoreCertificateErrors": true
  },
  "Hangfire": {
    "DisplayDashBoard": false,
    "CronForSendNotificationSubscribe": "0 7 * * *",
    "CronForSendEmailNotificator": "*/15 * * * *"
  },
  "AllowedHosts": "*",
  "RedirectUrl:": "*://**:**/"
}
